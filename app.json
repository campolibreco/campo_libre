{
  "expo": {
    "name": "Campo Libre CO",
    "description": "Get out there...",
    "slug": "campo-libre",
    "privacy": "unlisted",
    "sdkVersion": "27.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "0.0.48",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.campo.libre",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location will be used to help you pinpoint your GPS location when you're uploading a new campsite."
      }
    },
    "android": {
      "package": "com.campo.libre"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "campo-libre",
            "project": "campo-libre-colorado",
            "authToken": "auth_key_goes_here"
          }
        }
      ]
    }
  }
}


